<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/layout_linear">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Muse"
        android:onClick="launchMuse"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Synergy v2"
        android:onClick="launchSynergyV2"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alias List"
        android:onClick="launchAliasList"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Image List"
        android:onClick="launchImageList"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Audio List"
        android:onClick="launchAudioList"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Growth Areas"
        android:onClick="launchGrowthAreas"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TestMode"
        android:onClick="launchTestModeActivity"/>

</LinearLayout>
